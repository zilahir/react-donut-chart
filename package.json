{
  "name": "react-donut-chart",
  "version": "0.0.1",
  "description": "Creates donut charts in react",
  "main": "build/index.js",
  "peerDependencies": {
    "react": "^16.9.0"
  },
  "dependencies": {
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "prop-types": "^15.7.2",
